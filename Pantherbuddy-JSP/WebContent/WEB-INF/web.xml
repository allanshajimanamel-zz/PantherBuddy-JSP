<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <servlet>
    <description></description>
    <display-name>MainController</display-name>
    <servlet-name>MainController</servlet-name>
    <servlet-class>pantherbuddy.business.controller.control.MainController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MainController</servlet-name>
    <url-pattern>/MainController</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>PantherbuddyFilter</filter-name>
    <filter-class>pantherbuddy.business.controller.utility.PantherbuddyFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>PantherbuddyFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
</web-app>